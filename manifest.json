{
    "manifest_version": 3,
    "name": "Mouse Pattern Navigation",
    "version": "1.0",
    "description": "",
    "permissions": ["tabs"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  